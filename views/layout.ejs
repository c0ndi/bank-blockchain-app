<!DOCTYPE html>
<html>

<head>
  <title> <%=  %>  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <header class="header">
    <h1 class="header-title">Welcome in blockchain-bank-machine</h1>
  </header>
  <main class="main">
    <section class="main-left-section">
      <div class="names">
        <% usernames.forEach(username => { %> 
          <h1> <%= username %>  </h1>
        <% }) %> 
      </div>
      <div class="balance">
        <% balances.forEach(balance => { %> 
          <h1> <%= balance %>  $</h1>
        <% }) %> 
      </div>
    </section>
    <section class="main-right-section">
      <h1> <%= console.log(metaHero.users[0].balance); %> </h1>
    </section>
  </main>
</body>
  
</html>